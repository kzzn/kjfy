<view><view class="fixed"><cu-custom vue-id="1" isBack="{{true}}" bgColor="{{barColor}}" onVueInit="__l"><view slot="backText">返回</view><view slot="content">门卫登记</view></cu-custom></view><form><view class="flex solid-bottom justify-between cu-form-group"><view class="padding-sm margin-xs radius"><text class="{{(('cuIcon-peoplefill ')+' '+('text-'+bgColor))}}" style="font-size:30px;"></text><text>{{owner.name}}</text></view><view class="padding-sm margin-xs radius"><text class="{{(('cuIcon-phone ')+' '+('text-'+bgColor))}}" style="font-size:30px;"></text><text>{{owner.phone}}</text></view></view><view class="flex solid-bottom align-center bg-white"><view class="{{(('margin-xs radius cuIcon-card ')+' '+('text-'+bgColor))}}" style="font-size:30px;"></view><view class="margin-xs radius">{{owner.cardid}}</view></view><view class="flex solid-bottom align-center bg-white"><view class="{{(('margin-xs radius cuIcon-locationfill ')+' '+('text-'+bgColor))}}" style="font-size:30px;"></view><view class="margin-xs radius">{{owner.address}}</view></view><view class="cu-form-group margin-top hide" style="display:none;"><view class="title"><text class="{{(('cuIcon-timefill')+' '+('text-'+bgColor))}}"></text>登记时间</view><picker mode="time" value="{{time}}" start="09:01" end="21:01" data-event-opts="{{[['change',[['TimeChange',['$event']]]]]}}" onChange="__e"><view class="picker">{{''+time+''}}</view></picker></view><view class="cu-form-group margin-top"><view class="title"><text class="{{(('cuIcon-titles text-xxl')+' '+('text-'+bgColor))}}"></text>当前体温</view><input placeholder="当前体温" name="input" focus="{{temperaturefocus}}" data-event-opts="{{[['input',[['__set_model',['$0','temperature','$event',[]],['register']]]]]}}" value="{{register.temperature}}" onInput="__e"/>°C</view><view class="cu-form-group"><view class="title"><text class="{{(('cuIcon-title text-xxl')+' '+('text-'+bgColor))}}"></text>出行方式</view><radio-group data-event-opts="{{[['change',[['outtypeChange',['$event']]]]]}}" onChange="__e"><radio class="radio orange" checked="true" value="步行" name="triptype">步行</radio><radio class="radio orange" value="驾车" name="triptype">驾车</radio></radio-group></view><view class="cu-form-group"><view class="title"><text class="{{(('cuIcon-deliver_fill text-xxl')+' '+('text-'+bgColor))}}"></text>车牌号码</view><input placeholder="驾车出行的请填写车牌号码" name="car" focus="{{carnofocus}}" data-event-opts="{{[['input',[['__set_model',['$0','car','$event',[]],['register']]]]]}}" value="{{register.car}}" onInput="__e"/></view><view class="cu-form-group"><view class="title"><text class="{{(('cuIcon-edit text-xxl')+' '+('text-'+bgColor))}}"></text>备        注</view><textarea maxlength="-1" placeholder="请在此输入备注信息" data-event-opts="{{[['input',[['__set_model',['$0','remarks','$event',[]],['register']]]]]}}" value="{{register.remarks}}" onInput="__e"></textarea></view></form><view class="box"><view class="cu-bar tabbar bg-white shadow foot"><view data-event-opts="{{[['tap',[['saveRegister',['出']]]]]}}" class="submit" onTap="__e">外出登记</view><view data-event-opts="{{[['tap',[['saveRegister',['进']]]]]}}" class="{{(('submit')+' '+('bg-'+bgColor))}}" onTap="__e">进入登记</view></view></view></view>