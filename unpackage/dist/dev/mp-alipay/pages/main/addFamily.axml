<view><view class="fixed"><cu-custom vue-id="1" isBack="{{true}}" bgColor="{{barColor}}" onVueInit="__l"><view slot="backText">返回</view><view slot="content">添加家庭成员</view></cu-custom></view><form><view class="cu-form-group"><view class="title">姓名</view><input placeholder="业主姓名" name="input" focus="{{namefocus}}" data-event-opts="{{[['input',[['__set_model',['$0','name','$event',[]],['owner']]]]]}}" value="{{owner.name}}" onInput="__e"/><text class="{{(('cuIcon-peoplefill')+' '+('text-'+bgColor))}}"></text></view><view class="cu-form-group"><view class="title">手机号码</view><input placeholder="11位手机号码" name="input" focus="{{phonefocus}}" data-event-opts="{{[['input',[['__set_model',['$0','phone','$event',[]],['owner']]]]]}}" value="{{owner.phone}}" onInput="__e"/><view class="cu-capsule radius"><view class="{{(('cu-tag ')+' '+('text-'+bgColor))}}">+86</view><view class="{{(('cu-tag')+' '+('line-'+bgColor))}}">中国大陆</view></view></view><view class="cu-form-group"><view class="title">身份证号</view><input placeholder="身份证号" name="input" focus="{{idcardfocus}}" data-event-opts="{{[['input',[['__set_model',['$0','cardid','$event',[]],['owner']]]]]}}" value="{{owner.cardid}}" onInput="__e"/><text class="{{(('cuIcon-card')+' '+('text-'+bgColor))}}"></text></view></form><view class="box"><view class="cu-bar tabbar bg-white shadow foot"><view data-event-opts="{{[['tap',[['saveOwner']]]]}}" class="{{(('submit')+' '+('bg-'+bgColor))}}" onTap="__e">保存</view></view></view></view>