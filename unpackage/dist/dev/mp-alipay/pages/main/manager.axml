<view><cu-custom vue-id="1" isBack="{{true}}" bgColor="{{barColor}}" onVueInit="__l"><view slot="backText">返回</view><view slot="content">社区管理</view></cu-custom><swiper class="screen-swiper round-dot fixed" indicator-dots="{{true}}" circular="{{true}}" autoplay="{{true}}" interval="5000" duration="500"><block a:for="{{ads}}" a:for-item="item" a:for-index="index" a:key="index"><swiper-item data-isurl="{{item.isurl}}" data-url="{{item.url}}" data-id="{{item.id}}" data-event-opts="{{[['tap',[['openAds',['$event']]]]]}}" onTap="__e"><image src="{{item.titleImg}}" mode="aspectFill"></image></swiper-item></block></swiper><block a:if="{{warnmsg!=''?true:false}}"><uni-notice-bar vue-id="2" scrollable="{{true}}" single="{{true}}" text="{{warnmsg}}" onVueInit="__l"></uni-notice-bar></block><view class="VerticalBox"><scroll-view class="VerticalNav nav" style="{{'height:'+(scrollviewHeight+'px')+';'}}" id="datasview" scroll-y scroll-with-animation scroll-top="{{verticalNavTop}}"><block a:for="{{list}}" a:for-item="item" a:for-index="index" a:key="index"><view class="{{(('cu-item')+' '+(index==tabCur?'text-'+bgColor:''))}}" data-name="{{item.name}}" data-id="{{index}}" data-event-opts="{{[['tap',[['TabSelect',['$event']]]]]}}" onTap="__e">{{''+item.name+''}}</view></block></scroll-view><scroll-view class="VerticalMain" style="{{'height:'+(scrollviewHeight+'px')+';'}}" scroll-y scroll-with-animation scroll-into-view="{{'main-'+mainCur}}" data-event-opts="{{[['scroll',[['VerticalMain',['$event']]]]]}}" onScroll="__e"><block a:for="{{list}}" a:for-item="item" a:for-index="index" a:key="index"><view class="padding-top padding-lr" id="{{'main-'+index}}"><view class="cu-bar solid-bottom bg-white"><view class="action"><text class="{{(('cuIcon-title')+' '+('text-'+bgColor))}}"></text>{{item.name}}</view></view><view class="cu-list menu-avatar"><block a:for="{{item.units}}" a:for-item="unit" a:for-index="unitindex" a:key="unitindex"><view class="menuitem bg-white"><view class="content"><view class="text-grey margin-xs">{{unit.name}}</view><view class="grid col-4 padding-sm"><block a:for="{{unit.layers}}" a:for-item="storey" a:for-index="storeyindex" a:key="storeyindex"><view class="margin-tb-sm text-center"><button class="{{(('cu-btn round shadow sm margin-right-xs')+' '+('bg-'+bgColor))}}" data-floorindex="{{[index,unitindex,storeyindex]}}" data-build="{{item.name}}" data-unit="{{unit.name}}" data-storeyid="{{storey.id}}" data-storey="{{storey.name}}" data-event-opts="{{[['tap',[['openRooms',['$event']]]]]}}" onTap="__e">{{storey.name}}</button></view></block></view></view></view></block></view></view></block></scroll-view></view></view>