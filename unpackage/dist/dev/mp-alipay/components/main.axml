<view><cu-custom vue-id="1" bgColor="{{barColor}}" isBack="{{false}}" onVueInit="__l"><view slot="content">慧生活流控管家</view></cu-custom><swiper class="screen-swiper round-dot" id="adview" indicator-dots="{{true}}" circular="{{true}}" autoplay="{{true}}" interval="5000" duration="500"><block a:for="{{ads}}" a:for-item="item" a:for-index="index" a:key="index"><swiper-item><image src="{{item.titleImg}}" mode="aspectFill" data-isurl="{{item.isurl}}" data-url="{{item.url}}" data-id="{{item.id}}" data-event-opts="{{[['tap',[['openAds']]]]}}" onTap="__e"></image></swiper-item></block></swiper><block a:if="{{webUser.rolename=='物业'?true:false}}"><view class=" cu-list grid col-3 no-border"><view data-event-opts="{{[['tap',[['addOwner']]]]}}" class="cu-item  " onTap="__e"><view class="{{(('cu-avatar xl cu-avatar-btn round shadow')+' '+('bg-'+bgColor))}}">添加业主</view></view><view data-event-opts="{{[['tap',[['manager']]]]}}" class="cu-item " onTap="__e"><view class="{{(('cu-avatar xl cu-avatar-btn round shadow')+' '+('bg-'+bgColor))}}">小区管理</view></view><view data-event-opts="{{[['tap',[['location']]]]}}" class="cu-item " onTap="__e"><view class="{{(('cu-avatar xl cu-avatar-btn round shadow')+' '+('bg-'+bgColor))}}">签到</view></view></view></block><view><block a:if="{{webUser.rolename=='物业'?true:false}}"><view data-event-opts="{{[['tap',[['registerCode']]]]}}" class="fab-box fab rightBottom" onTap="__e"><view class="{{(('fab-circle bottom ')+' '+('bg-'+bgColor))}}" style="right:0px;"><text class="cuIcon-scan text-white xl" style="font-size:20px;"></text></view></view></block><block a:if="{{webUser.rolename=='业主'?true:false}}"><view data-event-opts="{{[['tap',[['addFamily']]]]}}" class="fab-box fab rightBottom" onTap="__e"><view class="{{(('fab-circle bottom')+' '+('bg-'+bgColor))}}" style="right:0px;"><text class="cuIcon-add text-white xl" style="font-size:20px;"></text></view></view></block></view><view class="cu-bar bg-white solid-bottom margin-top" id="titleview"><block a:if="{{webUser.rolename=='业主'?true:false}}"><view class="action"><text class="cuIcon-friendfamous text-orange "></text>家庭成员</view></block><block a:if="{{webUser.rolename=='物业'?true:false}}"><view class="action"><text class="cuIcon-title text-orange "></text>出入记录</view></block></view><block a:if="{{webUser.rolename=='业主'?true:false}}"><scroll-view class="page scroll-Y" style="{{'height:'+(scrollviewHeight+'px')+';'}}" id="datasview" scroll-y="true" data-event-opts="{{[['scrollToLower',[['pageToLower',['$event']]]]]}}" onScrollToLower="__e"><block a:for="{{owners}}" a:for-item="owner" a:for-index="index" a:key="index"><view class="cu-card article" id="{{owner.id}}"><view class="cu-item shadow" data-qrcode="{{owner.qrcode}}" data-userid="{{owner.id}}" data-event-opts="{{[['tap',[['openRecords',['$event']]]]]}}" onTap="__e"><view class="title"><view class="text-cut">{{owner.name+" ("+owner.cardid+")"}}</view></view><view class="content"><image style="width:100px;height:100px;" src="{{baseUrl+owner.qrcode}}" mode="aspectFit"></image><view class="desc"><view class="text-content"><view>{{"上次外出时间:"+owner.lastouttime+''}}</view><view>今日外出次数:<view class="{{(('cu-tag light sm round')+' '+('bg-'+bgColor))}}">{{owner.outcount+"次"}}</view></view><view>今日外出时长:<view class="{{(('cu-tag light sm round')+' '+('bg-'+bgColor))}}">{{owner.outtime}}</view></view></view><view><view class="{{(('cu-tag light sm round')+' '+('bg-'+bgColor))}}">{{owner.age+"岁"}}</view><view class="cu-tag bg-green light sm round">{{owner.sex}}</view></view></view></view></view></view></block><uni-load-more vue-id="2" status="{{loadStatus}}" onVueInit="__l"></uni-load-more></scroll-view></block><block a:if="{{webUser.rolename=='物业'?true:false}}"><scroll-view class="page scroll-Y" style="{{'height:'+(scrollviewHeight+'px')+';'}}" id="datasview" scroll-y="true" data-event-opts="{{[['scrollToLower',[['pageToLower',['$event']]]]]}}" onScrollToLower="__e"><view class="cu-list menu-avatar"><block a:for="{{recods}}" a:for-item="item" a:for-index="index" a:key="index"><view class="cu-item" data-name="{{item.name}}" data-id="{{item.id}}" data-event-opts="{{[['tap',[['openRecod',['$event']]]]]}}" onTap="__e"><view class="{{(('cu-avatar lg round ')+' '+(item.state=='出'?'bg-red':'bg-'+bgColor))}}">{{item.state}}</view><view class="content"><view class="text-grey"><view class="text-cut">{{item.name}}</view><view class="cu-tag round bg-orange sm">{{item.temperature+" ℃"}}</view></view><view class="text-gray text-sm flex"><view class="text-cut"><text class="cuIcon-location text-red  margin-right-xs"></text>{{''+item.address+''}}</view></view></view><view><view class="text-grey text-xs">{{item.createDate}}</view></view></view></block></view><uni-load-more vue-id="3" status="{{loadStatus}}" onVueInit="__l"></uni-load-more></scroll-view></block></view>