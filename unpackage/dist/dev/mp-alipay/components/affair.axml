<view><view class="fixed"><cu-custom vue-id="1" bgColor="{{barColor}}" isBack="{{false}}" onVueInit="__l"><view slot="content">慧生活流控管家</view></cu-custom></view><swiper class="screen-swiper round-dot fixed" indicator-dots="{{true}}" circular="{{true}}" autoplay="{{true}}" interval="5000" duration="500"><block a:for="{{ads}}" a:for-item="item" a:for-index="index" a:key="index"><swiper-item data-isurl="{{item.isurl}}" data-url="{{item.url}}" data-id="{{item.id}}" data-event-opts="{{[['tap',[['openAds',['$event']]]]]}}" onTap="__e"><image src="{{item.titleImg}}" mode="aspectFill"></image></swiper-item></block></swiper><view><view data-event-opts="{{[['tap',[['postAffair']]]]}}" class="fab-box fab rightBottom" onTap="__e"><view class="{{(('fab-circle bottom')+' '+('bg-'+bgColor))}}" style="right:0px;"><text class="cuIcon-upload text-white xl" style="font-size:20px;"></text></view></view></view><scroll-view class="bg-white nav" id="fiexdview" scroll-x scroll-with-animation scroll-left="{{scrollLeft}}"><view class="flex text-center"><view class="{{(('cu-item flex-sub')+' '+(TabCur=='2'?'text-'+bgColor+' cur':''))}}" data-index="{{2}}" index="2" data-event-opts="{{[['tap',[['tabSelect',['$event']]]]]}}" onTap="__e">未回复</view><view class="{{(('cu-item flex-sub')+' '+(TabCur=='3'?'text-'+bgColor+' cur':''))}}" data-index="{{3}}" index="3" data-event-opts="{{[['tap',[['tabSelect',['$event']]]]]}}" onTap="__e">已回复</view><view class="{{(('cu-item flex-sub')+' '+(TabCur=='4'?'text-'+bgColor+' cur':''))}}" data-index="{{4}}" index="4" data-event-opts="{{[['tap',[['tabSelect',['$event']]]]]}}" onTap="__e">已评价</view></view></scroll-view><scroll-view class="page" style="{{'height:'+(scrollviewHeight+'px')+';'}}" id="datasview" scroll-y><block a:for="{{affairs}}" a:for-item="entry" a:for-index="index" a:key="index"><view class="cu-card dynamic article" id="{{entry.id}}" data-id="{{entry.id}}" data-event-opts="{{[['tap',[['openEntry',['$event']]]]]}}" onTap="__e"><view class="cu-item shadow"><view class="flex justify-between"><view class="padding-sm margin-xs radius">{{"时间："+entry.createDate}}</view><view class="padding-sm margin-xs radius">{{"状态："+entry.status}}</view></view><view class="content"><view class="radius">内容：</view><view><rich-text nodes="{{entry.content}}"></rich-text></view></view></view></view></block><uni-load-more vue-id="2" status="{{loadStatus}}" onVueInit="__l"></uni-load-more></scroll-view></view>