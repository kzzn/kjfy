<view><view class="fixed"><cu-custom vue-id="1" bgColor="{{barColor}}" isBack="{{false}}" onVueInit="__l"><view slot="content">慧生活流控管家</view></cu-custom></view><swiper class="screen-swiper round-dot fixed" indicator-dots="{{true}}" circular="{{true}}" autoplay="{{true}}" interval="5000" duration="500"><block a:for="{{ads}}" a:for-item="item" a:for-index="index" a:key="index"><swiper-item data-isurl="{{item.isurl}}" data-url="{{item.url}}" data-id="{{item.id}}" data-event-opts="{{[['tap',[['openAds',['$event']]]]]}}" onTap="__e"><image src="{{item.titleImg}}" mode="aspectFill"></image></swiper-item></block></swiper><scroll-view class="page" style="{{'height:'+(scrollviewHeight+'px')+';'}}" id="datasview" scroll-y><block a:for="{{articles}}" a:for-item="article" a:for-index="index" a:key="index"><view class="cu-card dynamic no-card article" id="{{article.id}}" data-id="{{article.id}}" data-event-opts="{{[['tap',[['openActicle',['$event']]]]]}}" onTap="__e"><view class="cu-item shadow"><view class="title"><view class="text-cut">{{article.title+''}}</view></view><view class="content"><image src="{{article.titleImg==''?defaultimg:article.titleImg}}" mode="aspectFill"></image><view class="desc"><view class="text-content"><rich-text nodes="{{article.content}}"></rich-text></view><view><view class="{{(('cu-tag light sm round')+' '+('bg-'+bgColor))}}">{{article.createDate+" 发布"}}</view></view></view></view></view></view></block><uni-load-more vue-id="2" status="{{loadStatus}}" onVueInit="__l"></uni-load-more></scroll-view></view>