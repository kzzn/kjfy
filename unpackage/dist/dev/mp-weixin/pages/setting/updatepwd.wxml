<view><view class="fixed"><cu-custom vue-id="1" isBack="{{true}}" bgColor="bg-shadeTop text-white" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">密码修改</view></cu-custom></view><view class="cu-form-group"><view class="title"><text class="cuIcon-edit text-brown text-xxl"></text>原密码</view><input placeholder="原密码" name="oldpwd" data-event-opts="{{[['input',[['__set_model',['','oldpwd','$event',[]]]]]]}}" value="{{oldpwd}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title"><text class="cuIcon-new text-brown text-xxl"></text>新密码</view><input password="true" placeholder="新密码" name="newpwd" data-event-opts="{{[['input',[['__set_model',['','newpwd','$event',[]]]]]]}}" value="{{newpwd}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title"><text class="cuIcon-sound text-brown text-xxl"></text>确认密码</view><input password="true" placeholder="确认密码" name="surepwd" data-event-opts="{{[['input',[['__set_model',['','surepwd','$event',[]]]]]]}}" value="{{surepwd}}" bindinput="__e"/></view><view class="padding flex flex-direction"><button class="cu-btn bg-brown margin-tb-sm lg" loading="{{isupdate?true:false}}" data-event-opts="{{[['tap',[['updatepwd',['$event']]]]]}}" bindtap="__e">确认修改</button></view></view>