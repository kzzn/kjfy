<view><cu-custom vue-id="1" isBack="{{true}}" bgColor="bg-shadeTop text-white" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">{{"注册"+requestParams.registertype}}</view></cu-custom><form style="margin-bottom:85px;"><block wx:if="{{scantype==''?true:false}}"><view class="cu-form-group"><view class="title">区划</view><picker mode="multiSelector" value="{{areaIndex}}" range="{{areaArray}}" data-event-opts="{{[['change',[['AreaChange',['$event']]]],['columnchange',[['AreaColumnChange',['$event']]]]]}}" bindchange="__e" bindcolumnchange="__e"><view class="picker">{{''+areaArray[0][areaIndex[0]]+"，"+areaArray[1][areaIndex[1]]+"，"+areaArray[2][areaIndex[2]]+''}}</view></picker><text class="cuIcon-locationfill text-brown"></text></view></block><block wx:if="{{scantype==''?true:false}}"><view class="cu-form-group"><view class="title">街道</view><picker value="{{streetindex}}" range="{{streetPicker}}" range-key="{{name}}" data-event-opts="{{[['change',[['streetChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(streetindex>-1?streetPicker[streetindex]:'请选择社区')+''}}</view></picker><text class="cuIcon-locationfill text-brown"></text></view></block><block wx:if="{{scantype==''?true:false}}"><view class="cu-form-group"><view class="title">社区</view><picker value="{{communityindex}}" range="{{communityPicker}}" range-key="{{name}}" data-event-opts="{{[['change',[['communityChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(communityindex>-1?communityPicker[communityindex]:'请选择社区')+''}}</view></picker><text class="cuIcon-locationfill text-brown"></text></view></block><block wx:if="{{requestParams.registertype=='业主'&&scantype==''?true:false}}"><view class="cu-form-group"><view class="title">小区</view><picker value="{{villageindex}}" range="{{villagePicker}}" range-key="{{name}}" data-event-opts="{{[['change',[['villageChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(villageindex>-1?villagePicker[villageindex]:'请选择社区')+''}}</view></picker><text class="cuIcon-locationfill text-brown"></text></view></block><block wx:if="{{requestParams.registertype=='业主'?true:false}}"><view class="cu-form-group"><view class="title">楼层</view><picker mode="multiSelector" value="{{multiIndex}}" range="{{multiArray}}" data-event-opts="{{[['change',[['MultiChange',['$event']]]],['columnchange',[['MultiColumnChange',['$event']]]]]}}" bindchange="__e" bindcolumnchange="__e"><view class="picker">{{''+multiArray[0][multiIndex[0]]+"，"+multiArray[1][multiIndex[1]]+"，"+multiArray[2][multiIndex[2]]+''}}</view></picker><text class="cuIcon-locationfill text-brown"></text></view></block><block wx:if="{{requestParams.registertype=='业主'?true:false}}"><view class="cu-form-group"><view class="title">门牌</view><picker value="{{roomindex}}" range="{{roomsPicker}}" range-key="{{name}}" data-event-opts="{{[['change',[['RomeChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(roomindex>-1?roomsPicker[roomindex]:'居住房屋门牌号')+''}}</view></picker><text class="cuIcon-newsfill text-brown"></text></view></block><view class="cu-form-group"><block wx:if="{{requestParams.registertype=='物业'?true:false}}"><view class="title">小区名字</view></block><block wx:if="{{requestParams.registertype=='业主'?true:false}}"><view class="title">您的姓名</view></block><input placeholder="请在此输入名字" name="input" focus="{{namefocus}}" data-event-opts="{{[['input',[['__set_model',['$0','name','$event',[]],['entry']]]]]}}" value="{{entry.name}}" bindinput="__e"/><text class="cuIcon-peoplefill text-brown"></text></view><view class="cu-form-group"><view class="title">联系电话</view><input placeholder="{{requestParams.registertype=='物业'?'位手机号码':'手机号码作为登录账号'}}" name="input" focus="{{phonefocus}}" data-event-opts="{{[['input',[['__set_model',['$0','phone','$event',[]],['entry']]]]]}}" value="{{entry.phone}}" bindinput="__e"/><view class="cu-capsule radius"><view class="cu-tag text-brown ">+86</view><view class="cu-tag line-brown">中国大陆</view></view></view><block wx:if="{{requestParams.registertype=='业主'?true:false}}"><view class="cu-form-group"><view class="title">身份证号</view><input placeholder="用于检测账号的唯一性" name="cardid" data-event-opts="{{[['input',[['__set_model',['$0','cardid','$event',[]],['entry']]]]]}}" value="{{entry.cardid}}" bindinput="__e"/><text class="cuIcon-new text-brown text-xxl"></text></view></block><block wx:if="{{requestParams.registertype=='业主'?true:false}}"><view class="cu-form-group"><view class="title">登录密码默认手机号后6位</view></view></block><block wx:if="{{requestParams.registertype=='物业'?true:false}}"><view class="cu-form-group"><view class="title"><text class="cuIcon-new text-brown text-xxl"></text>登录账号</view><input placeholder="请输入唯一登录账号,字母加数字" name="loginName" data-event-opts="{{[['input',[['__set_model',['$0','loginName','$event',[]],['entry']]]]]}}" value="{{entry.loginName}}" bindinput="__e"/></view></block><block wx:if="{{requestParams.registertype=='物业'?true:false}}"><view class="cu-form-group"><view class="title"><text class="cuIcon-new text-brown text-xxl"></text>登录密码</view><input password="true" placeholder="输入您的登录密码" name="password" data-event-opts="{{[['input',[['__set_model',['$0','password','$event',[]],['entry']]]]]}}" value="{{entry.password}}" bindinput="__e"/></view></block><block wx:if="{{requestParams.registertype=='物业'?true:false}}"><view class="cu-form-group"><view class="title"><text class="cuIcon-sound text-brown text-xxl"></text>确认密码</view><input password="true" placeholder="确认密码" name="surepassword" data-event-opts="{{[['input',[['__set_model',['$0','surepassword','$event',[]],['entry']]]]]}}" value="{{entry.surepassword}}" bindinput="__e"/></view></block><block wx:if="{{requestParams.registertype=='物业'?true:false}}"><view class="cu-bar bg-white solid-bottom margin-top" id="titleview"><view class="action"><text class="cuIcon-global text-orange "></text>建筑信息</view></view></block><block wx:if="{{requestParams.registertype=='物业'?true:false}}"><view class="cu-form-group"><view class="title">建筑数</view><input placeholder="小区总计楼栋数,整数型数字" name="floor" data-event-opts="{{[['input',[['__set_model',['$0','floor','$event',[]],['entry']]]]]}}" value="{{entry.floor}}" bindinput="__e"/></view></block><block wx:if="{{requestParams.registertype=='物业'?true:false}}"><view class="cu-form-group"><view class="title">单元数</view><input placeholder="每栋楼的最多单元数,整数型数字" data-event-opts="{{[['input',[['__set_model',['$0','unit','$event',[]],['entry']]]]]}}" value="{{entry.unit}}" bindinput="__e"/></view></block><block wx:if="{{requestParams.registertype=='物业'?true:false}}"><view class="cu-form-group"><view class="title">楼层数</view><input placeholder="每个单元的最高楼层,整数型数字" data-event-opts="{{[['input',[['__set_model',['$0','layer','$event',[]],['entry']]]]]}}" value="{{entry.layer}}" bindinput="__e"/></view></block><block wx:if="{{requestParams.registertype=='物业'?true:false}}"><view class="cu-form-group" style="margin-bottom:50px;"><view class="title">户数</view><input placeholder="每层户数,整数型数字" data-event-opts="{{[['input',[['__set_model',['$0','room','$event',[]],['entry']]]]]}}" value="{{entry.room}}" bindinput="__e"/></view></block></form><view class="box"><view class="cu-bar tabbar bg-white shadow foot"><view data-event-opts="{{[['tap',[['register',['$event']]]]]}}" class="bg-brown submit" bindtap="__e">确认注册</view></view></view></view>