<view><cu-custom vue-id="1" bgColor="bg-shadeTop  text-white" isBack="{{false}}" bind:__l="__l" vue-slots="{{['content']}}"><view slot="content">用户登录</view></cu-custom><view class="cu-form-group"><view class="title">登录账户</view><input placeholder="请填写您的账户" name="loginno" focus="true" confirm-type="go" data-event-opts="{{[['input',[['getloginno',['$event']]]]]}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">登录密码</view><input placeholder="请输入登录密码" name="loginpwd" password="true" confirm-type="go" data-event-opts="{{[['input',[['getloginpwd',['$event']]]]]}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">是否自动登录</view><switch class="{{[autologin?'checked':'']}}" checked="{{autologin?true:false}}" data-event-opts="{{[['change',[['autologinC',['$event']]]]]}}" bindchange="__e"></switch></view><view class="padding flex flex-direction"><button class="cu-btn bg-brown margin-tb-sm lg" loading="{{islogin?true:false}}" data-event-opts="{{[['tap',[['login',['$event']]]]]}}" bindtap="__e">登录</button></view><view class="text-center"><button data-event-opts="{{[['tap',[['registerCode',['$event']]]]]}}" class="cu-btn" bindtap="__e"><text class="cuIcon-qrcode"></text>扫码注册</button></view><view class="text-center"><button data-event-opts="{{[['tap',[['register',['物业']]]]]}}" class="cu-btn" bindtap="__e">(物业小区注册</button><button data-event-opts="{{[['tap',[['register',['业主']]]]]}}" class="cu-btn" bindtap="__e">业主注册)</button></view><view class="text-center margin-top">Copyright© 2009-2020 nmghl.com 内蒙古汇联</view><view class="text-center">联系电话：0471-3284826</view></view>