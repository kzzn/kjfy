ew><view class="margin-xs radius">{{recode.state}}</view></view><view class="cu-form-group"><view class="title"><text class="{{(('cuIcon-edit text-xxl')+' '+('text-'+bgColor))}}"></text>备注</view><view class="margin-xs radius">{{recode.remarks}}</view></view></form></view>