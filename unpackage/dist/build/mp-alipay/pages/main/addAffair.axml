<view><cu-custom vue-id="1" isBack="{{true}}" bgColor="{{barColor}}" onVueInit="__l"><view slot="backText">返回</view><view slot="content">诉求申报</view></cu-custom><view class="flex justify-between"><view class="padding-sm margin-xs radius">{{"申报人："+webUser.username}}</view><view class="padding-sm margin-xs radius">{{"申报时间："+ctime}}</view></view><form><view class="bg-white"><view class="action"><text class="cuIcon-comment text-orange "></text>诉求内容：</view><view class="cu-form-group solid-top"><textarea maxlength="-1" placeholder="请在此输入您的诉求" data-event-opts="{{[['input',[['__set_model',['','content','$event',[]]]]]]}}" value="{{content}}" onInput="__e"></textarea></view></view></form><view class="padding flex flex-direction"><button class="{{(('cu-btn margin-tb-sm lg')+' '+('bg-'+bgColor))}}" loading="{{islogin?true:false}}" data-event-opts="{{[['tap',[['postAffair',['$event']]]]]}}" onTap="__e">上报</button></view></view>