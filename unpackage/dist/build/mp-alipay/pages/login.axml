<view><cu-custom vue-id="1" bgColor="{{barColor}}" isBack="{{false}}" onVueInit="__l"><view slot="content">用户登录</view></cu-custom><view class="cu-form-group"><view class="title">登录账户</view><input placeholder="请填写您的账户" name="loginno" focus="true" confirm-type="go" data-event-opts="{{[['input',[['getloginno',['$event']]]]]}}" onInput="__e"/></view><view class="cu-form-group"><view class="title">登录密码</view><input placeholder="请输入登录密码" name="loginpwd" password="true" confirm-type="go" data-event-opts="{{[['input',[['getloginpwd',['$event']]]]]}}" onInput="__e"/></view><view class="cu-form-group"><view class="title">是否自动登录</view><switch class="{{(autologin?'checked':'')}}" checked="{{autologin?true:false}}" data-event-opts="{{[['change',[['autologinC',['$event']]]]]}}" onChange="__e"></switch></view><view class="padding flex flex-direction"><button class="{{(('cu-btn margin-tb-sm lg')+' '+('bg-'+bgColor))}}" loading="{{islogin?true:false}}" data-event-opts="{{[['tap',[['login',['$event']]]]]}}" onTap="__e">登录</button></view><view class="text-center"><button data-event-opts="{{[['tap',[['openArticle',['$event']]]]]}}" class="cu-btn" onTap="__e"><text class="cuIcon-file"></text>使用手册</button></view><view class="text-center"><button data-event-opts="{{[['tap',[['registerCode',['$event']]]]]}}" class="cu-btn" onTap="__e">(<text class="cuIcon-qrcode"></text>扫码注册</button><button data-event-opts="{{[['tap',[['register',['物业']]]]]}}" class="cu-btn" onTap="__e"><text class="cuIcon-friendadd"></text>小区注册</button><button data-event-opts="{{[['tap',[['register',['业主']]]]]}}" class="cu-btn" onTap="__e"><text class="cuIcon-friendaddfill"></text>业主注册)</button></view><view class="text-center margin-top">Copyright© 2009-2020 nmghl.com 内蒙古汇联</view><view class="text-center">联系电话：0471-3284826</view></view>