<view><cu-custom vue-id="1" isBack="{{true}}" bgColor="{{barColor}}" onVueInit="__l"><view slot="backText">返回</view><view slot="content">{{"注册"+requestParams.registertype}}</view></cu-custom><form style="margin-bottom:85px;"><block a:if="{{scantype==''?true:false}}"><view class="cu-form-group"><view class="title">区划</view><picker value="{{provinceIndex}}" range="{{areaArray[0]}}" data-event-opts="{{[['change',[['provinceChange',['$event']]]]]}}" onChange="__e"><view class="uni-input">{{areaArray[0][provinceIndex]}}</view></picker><picker value="{{cityIndex}}" range="{{areaArray[1]}}" data-event-opts="{{[['change',[['cityChange',['$event']]]]]}}" onChange="__e"><view class="uni-input">{{areaArray[1][cityIndex]}}</view></picker><picker value="{{countyIndex}}" range="{{areaArray[2]}}" data-event-opts="{{[['change',[['countyChange',['$event']]]]]}}" onChange="__e"><view class="uni-input">{{areaArray[2][countyIndex]}}</view></picker><text class="{{(('cuIcon-locationfill')+' '+('text-'+bgColor))}}"></text></view></block><block a:if="{{scantype==''?true:false}}"><view class="cu-form-group"><view class="title">街道</view><picker value="{{streetindex}}" range="{{streetPicker}}" range-key="{{name}}" data-event-opts="{{[['change',[['streetChange',['$event']]]]]}}" onChange="__e"><view class="picker">{{''+(streetindex>-1?streetPicker[streetindex]:'请选择社区')+''}}</view></picker><text class="{{(('cuIcon-locationfill')+' '+('text-'+bgColor))}}"></text></view></block><block a:if="{{scantype==''?true:false}}"><view class="cu-form-group"><view class="title">社区</view><picker value="{{communityindex}}" range="{{communityPicker}}" range-key="{{name}}" data-event-opts="{{[['change',[['communityChange',['$event']]]]]}}" onChange="__e"><view class="picker">{{''+(communityindex>-1?communityPicker[communityindex]:'请选择社区')+''}}</view></picker><text class="{{(('cuIcon-locationfill')+' '+('text-'+bgColor))}}"></text></view></block><block a:if="{{requestParams.registertype=='业主'&&scantype==''?true:false}}"><view class="cu-form-group"><view class="title">小区</view><picker value="{{villageindex}}" range="{{villagePicker}}" range-key="{{name}}" data-event-opts="{{[['change',[['villageChange',['$event']]]]]}}" onChange="__e"><view class="picker">{{''+(villageindex>-1?villagePicker[villageindex]:'请选择社区')+''}}</view></picker><text class="{{(('cuIcon-locationfill')+' '+('text-'+bgColor))}}"></text></view></block><block a:if="{{requestParams.registertype=='业主'?true:false}}"><view class="cu-form-group"><view class="title">楼层</view><picker value="{{floorIndex}}" range="{{multiArray[0]}}" data-event-opts="{{[['change',[['floorChange',['$event']]]]]}}" onChange="__e"><view class="uni-input">{{multiArray[0][floorIndex]}}</view></picker><picker value="{{unitsIndex}}" range="{{multiArray[1]}}" data-event-opts="{{[['change',[['unitsChange',['$event']]]]]}}" onChange="__e"><view class="uni-input">{{multiArray[1][unitsIndex]}}</view></picker><picker value="{{layersIndex}}" range="{{multiArray[2]}}" data-event-opts="{{[['change',[['layersChange',['$event']]]]]}}" onChange="__e"><view class="uni-input">{{multiArray[2][layersIndex]}}</view></picker><text class="{{(('cuIcon-locationfill')+' '+('text-'+bgColor))}}"></text></view></block><block a:if="{{requestParams.registertype=='业主'?true:false}}"><view class="cu-form-group"><view class="title">门牌</view><picker value="{{roomindex}}" range="{{roomsPicker}}" range-key="{{name}}" data-event-opts="{{[['change',[['RomeChange',['$event']]]]]}}" onChange="__e"><view class="picker">{{''+(roomindex>-1?roomsPicker[roomindex]:'居住房屋门牌号')+''}}</view></picker><text class="{{(('cuIcon-newsfill')+' '+('text-'+bgColor))}}"></text></view></block><view class="cu-form-group"><block a:if="{{requestParams.registertype=='物业'?true:false}}"><view class="title">小区名字</view></block><block a:if="{{requestParams.registertype=='业主'?true:false}}"><view class="title">您的姓名</view></block><input placeholder="请在此输入名字" name="input" focus="{{namefocus}}" data-event-opts="{{[['input',[['__set_model',['$0','name','$event',[]],['entry']]]]]}}" value="{{entry.name}}" onInput="__e"/><text class="{{(('cuIcon-peoplefill')+' '+('text-'+bgColor))}}"></text></view><view class="cu-form-group"><view class="title">联系电话</view><input placeholder="{{requestParams.registertype=='物业'?'位手机号码':'手机号码作为登录账号'}}" name="input" focus="{{phonefocus}}" data-event-opts="{{[['input',[['__set_model',['$0','phone','$event',[]],['entry']]]]]}}" value="{{entry.phone}}" onInput="__e"/><view class="cu-capsule radius"><view class="{{(('cu-tag')+' '+('text-'+bgColor))}}">+86</view><view class="{{(('cu-tag')+' '+('line-'+bgColor))}}">中国大陆</view></view></view><block a:if="{{requestParams.registertype=='业主'?true:false}}"><view class="cu-form-group"><view class="title">身份证号</view><input placeholder="用于检测账号的唯一性" name="cardid" data-event-opts="{{[['input',[['__set_model',['$0','cardid','$event',[]],['entry']]]]]}}" value="{{entry.cardid}}" onInput="__e"/><text class="{{(('cuIcon-new text-xxl')+' '+('text-'+bgColor))}}"></text></view></block><block a:if="{{requestParams.registertype=='业主'?true:false}}"><view class="cu-form-group"><view class="title">登录密码默认手机号后6位</view></view></block><block a:if="{{requestParams.registertype=='物业'?true:false}}"><view class="cu-form-group"><view class="title"><text class="{{(('cuIcon-new text-xxl')+' '+('text-'+bgColor))}}"></text>登录账号</view><input placeholder="请输入唯一登录账号,字母加数字" name="loginName" data-event-opts="{{[['input',[['__set_model',['$0','loginName','$event',[]],['entry']]]]]}}" value="{{entry.loginName}}" onInput="__e"/></view></block><block a:if="{{requestParams.registertype=='物业'?true:false}}"><view class="cu-form-group"><view class="title"><text class="{{(('cuIcon-new text-xxl')+' '+('text-'+bgColor))}}"></text>登录密码</view><input password="true" placeholder="输入您的登录密码" name="password" data-event-opts="{{[['input',[['__set_model',['$0','password','$event',[]],['entry']]]]]}}" value="{{entry.password}}" onInput="__e"/></view></block><block a:if="{{requestParams.registertype=='物业'?true:false}}"><view class="cu-form-group"><view class="title"><text class="{{(('cuIcon-sound text-xxl')+' '+('text-'+bgColor))}}"></text>确认密码</view><input password="true" placeholder="确认密码" name="surepassword" data-event-opts="{{[['input',[['__set_model',['$0','surepassword','$event',[]],['entry']]]]]}}" value="{{entry.surepassword}}" onInput="__e"/></view></block><block a:if="{{requestParams.registertype=='物业'?true:false}}"><view class="cu-bar bg-white solid-bottom margin-top" id="titleview"><view class="action"><text class="cuIcon-global text-orange "></text>建筑信息</view></view></block><block a:if="{{requestParams.registertype=='物业'?true:false}}"><view class="cu-form-group"><view class="title">建筑数</view><input placeholder="小区总计楼栋数,整数型数字" name="floor" data-event-opts="{{[['input',[['__set_model',['$0','floor','$event',[]],['entry']]]]]}}" value="{{entry.floor}}" onInput="__e"/></view></block><block a:if="{{requestParams.registertype=='物业'?true:false}}"><view class="cu-form-group"><view class="title">单元数</view><input placeholder="每栋楼的最多单元数,整数型数字" data-event-opts="{{[['input',[['__set_model',['$0','unit','$event',[]],['entry']]]]]}}" value="{{entry.unit}}" onInput="__e"/></view></block><block a:if="{{requestParams.registertype=='物业'?true:false}}"><view class="cu-form-group"><view class="title">楼层数</view><input placeholder="每个单元的最高楼层,整数型数字" data-event-opts="{{[['input',[['__set_model',['$0','layer','$event',[]],['entry']]]]]}}" value="{{entry.layer}}" onInput="__e"/></view></block><block a:if="{{requestParams.registertype=='物业'?true:false}}"><view class="cu-form-group" style="margin-bottom:50px;"><view class="title">户数</view><input placeholder="每层户数,整数型数字" data-event-opts="{{[['input',[['__set_model',['$0','room','$event',[]],['entry']]]]]}}" value="{{entry.room}}" onInput="__e"/></view></block></form><view class="box"><view class="cu-bar tabbar bg-white shadow foot"><view data-event-opts="{{[['tap',[['register',['$event']]]]]}}" class="{{(('submit')+' '+('bg-'+bgColor))}}" onTap="__e">确认注册</view></view></view></view>