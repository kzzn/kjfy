<view><cu-custom vue-id="1" bgColor="{{barColor}}" isBack="{{true}}" onVueInit="__l"><view slot="backText">返回</view><view slot="content">诉求详情</view></cu-custom><view class="cu-card dynamic"><view class="cu-item shadow"><view class="cu-list menu-avatar"><view class="cu-item"><view class="{{(('cu-avatar xs cu-avatar-btn round shadow')+' '+('bg-'+bgColor))}}">诉</view><view class="content flex-sub"><view class="text-gray text-sm flex justify-between">{{''+entry.createDate+''}}</view></view></view></view><view class="text-content">{{''+entry.content+''}}</view><view class="cu-list menu-avatar comment solids-top"><view class="cu-item"><view class="cu-avatar xs cu-avatar-btn round bg-gradual-green shadow">回</view><block a:if="{{entry.status=='未回复'}}"><view class="content">未回复</view></block><block a:if="{{entry.status!='未回复'}}"><view class="content"><view class="text-grey">{{entry.receiptDate}}</view><view class="text-gray text-content text-df">{{''+entry.receipt+''}}</view></view></block></view></view></view></view><block a:if="{{entry.status=='已评价'||entry.status=='已回复'?true:false}}"><view class="cu-bar bg-white solid-bottom" id="titleview"><view class="action"><text class="cuIcon-title text-orange "></text>处理结果</view></view></block><block a:if="{{entry.status=='已评价'||entry.status=='已回复'?true:false}}"><view class="cu-card no-card dynamic margin-top"><view class="cu-item shadow"><block a:if="{{entry.status=='已回复'?true:false}}"><form><view class="cu-form-group"><view class="title"><text class="{{(('cuIcon-edit text-xxl')+' '+('text'+bgColor))}}"></text>评分</view><uni-rate vue-id="2" value="{{ratevalue}}" data-event-opts="{{[['^change',[['onChange']]]]}}" onChange="__e" onVueInit="__l"></uni-rate></view><view class="cu-form-group"><view class="title"><text class="{{(('cuIcon-edit text-xxl')+' '+('text-'+bgColor))}}"></text>内容</view><textarea maxlength="-1" placeholder="请在此输入您的评价内容" data-event-opts="{{[['input',[['__set_model',['$0','opinion','$event',[]],['entry']]]]]}}" value="{{entry.opinion}}" onInput="__e"></textarea></view><view class="cu-form-group"><button data-event-opts="{{[['tap',[['rated',['未解决']]]]]}}" class="cu-btn round shadow bg-red" onTap="__e">未解决</button><button data-event-opts="{{[['tap',[['rated',['已解决']]]]]}}" class="{{(('cu-btn round shadow')+' '+('bg-'+bgColor))}}" onTap="__e">已解决</button></view></form></block><block a:if="{{entry.status=='已评价'?true:false}}"><form><view class="cu-form-group"><view class="title"><text class="{{(('cuIcon-edit text-xxl')+' '+('text-'+bgColor))}}"></text>评分</view><uni-rate vue-id="3" value="{{entry.satisfaction}}" onVueInit="__l"></uni-rate></view><view class="cu-form-group"><view class="title"><text class="{{(('cuIcon-edit text-xxl')+' '+('text-'+bgColor))}}"></text>内容</view><view class="content">{{entry.opinion}}</view></view><view class="cu-form-group"><view class="title"><text class="{{(('cuIcon-edit text-xxl')+' '+('text-'+bgColor))}}"></text>结果</view><view class="content">{{entry.solved}}</view></view></form></block></view></view></block></view>