<view><view class="fixed"><cu-custom vue-id="1" bgColor="bg-shadeTop  text-white" isBack="{{false}}" bind:__l="__l" vue-slots="{{['content']}}"><view slot="content">慧生活流控管家</view></cu-custom></view><swiper class="screen-swiper round-dot fixed" indicator-dots="{{true}}" circular="{{true}}" autoplay="{{true}}" interval="5000" duration="500"><block wx:for="{{ads}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item data-isurl="{{item.isurl}}" data-url="{{item.url}}" data-id="{{item.id}}" data-event-opts="{{[['tap',[['openAds',['$event']]]]]}}" bindtap="__e"><image src="{{item.titleImg}}" mode="aspectFill"></image></swiper-item></block></swiper><scroll-view class="page" style="{{'height:'+(scrollviewHeight+'px')+';'}}" id="datasview" scroll-y><block wx:for="{{articles}}" wx:for-item="article" wx:for-index="index" wx:key="index"><view class="cu-card dynamic no-card article" id="{{article.id}}" data-id="{{article.id}}" data-event-opts="{{[['tap',[['openActicle',['$event']]]]]}}" bindtap="__e"><view class="cu-item shadow"><view class="title"><view class="text-cut">{{article.title+''}}</view></view><view class="content"><image src="{{article.titleImg==''?defaultimg:article.titleImg}}" mode="aspectFill"></image><view class="desc"><view class="text-content"><rich-text nodes="{{article.content}}"></rich-text></view><view><view class="cu-tag bg-brown light sm round">{{article.createDate+" 发布"}}</view></view></view></view></view></view></block><uni-load-more vue-id="2" status="{{loadStatus}}" bind:__l="__l"></uni-load-more></scroll-view></view>