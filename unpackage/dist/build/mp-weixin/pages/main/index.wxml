<view><cu-custom vue-id="1" bgColor="bg-shadeTop  text-white" isBack="{{false}}" bind:__l="__l" vue-slots="{{['content']}}"><view slot="content">慧生活流控管家</view></cu-custom><swiper class="screen-swiper round-dot" id="adview" indicator-dots="{{true}}" circular="{{true}}" autoplay="{{true}}" interval="5000" duration="500"><block wx:for="{{ads}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item data-isurl="{{item.isurl}}" data-url="{{item.url}}" data-id="{{item.id}}" data-event-opts="{{[['tap',[['openAds',['$event']]]]]}}" bindtap="__e"><image src="{{item.titleImg}}" mode="aspectFill"></image></swiper-item></block></swiper><block wx:if="{{webUser.rolename=='物业'?true:false}}"><view class=" cu-list grid col-3 no-border"><view data-event-opts="{{[['tap',[['addOwner']]]]}}" class="cu-item  " bindtap="__e"><view class="cu-avatar xl cu-avatar-btn round bg-brown shadow">添加业主</view></view><view data-event-opts="{{[['tap',[['manager']]]]}}" class="cu-item " bindtap="__e"><view class="cu-avatar xl cu-avatar-btn round bg-brown shadow">小区管理</view></view><view data-event-opts="{{[['tap',[['location']]]]}}" class="cu-item " bindtap="__e"><view class="cu-avatar xl cu-avatar-btn round bg-brown shadow">签到</view></view></view></block><view><block wx:if="{{webUser.rolename=='物业'?true:false}}"><view data-event-opts="{{[['tap',[['registerCode']]]]}}" class="fab-box fab rightBottom" bindtap="__e"><view class="fab-circle bottom bg-brown" style="right:0px;"><text class="cuIcon-scan text-white xl" style="font-size:20px;"></text></view></view></block><block wx:if="{{webUser.rolename=='业主'?true:false}}"><view data-event-opts="{{[['tap',[['addFamily']]]]}}" class="fab-box fab rightBottom" bindtap="__e"><view class="fab-circle bottom bg-brown" style="right:0px;"><text class="cuIcon-add text-white xl" style="font-size:20px;"></text></view></view></block></view><view class="cu-bar bg-white solid-bottom margin-top" id="titleview"><block wx:if="{{webUser.rolename=='业主'?true:false}}"><view class="action"><text class="cuIcon-friendfamous text-orange "></text>家庭成员</view></block><block wx:if="{{webUser.rolename=='物业'?true:false}}"><view class="action"><text class="cuIcon-title text-orange "></text>出入记录</view></block></view><block wx:if="{{webUser.rolename=='业主'?true:false}}"><scroll-view class="page scroll-Y" style="{{'height:'+(scrollviewHeight+'px')+';'}}" id="datasview" scroll-y="true" data-event-opts="{{[['scrolltolower',[['pageToLower',['$event']]]]]}}" bindscrolltolower="__e"><block wx:for="{{owners}}" wx:for-item="owner" wx:for-index="index" wx:key="index"><view class="cu-card article" id="{{owner.id}}"><view class="cu-item shadow" data-qrcode="{{owner.qrcode}}" data-userid="{{owner.id}}" data-event-opts="{{[['tap',[['openRecords',['$event']]]]]}}" bindtap="__e"><view class="title"><view class="text-cut">{{owner.name+" ("+owner.cardid+")"}}</view></view><view class="content"><image style="width:120px;" src="{{baseUrl+owner.qrcode}}" mode="aspectFit"></image><view class="desc"><view class="text-content"><view>{{"上次外出时间:"+owner.lastouttime+''}}</view><view>今日外出次数:<view class="cu-tag bg-brown light sm round">{{owner.outcount+"次"}}</view></view><view>今日外出时长:<view class="cu-tag bg-brown light sm round">{{owner.outtime}}</view></view></view><view><view class="cu-tag bg-brown light sm round">{{owner.age+"岁"}}</view><view class="cu-tag bg-green light sm round">{{owner.sex}}</view></view></view></view></view></view></block><uni-load-more vue-id="2" status="{{loadStatus}}" bind:__l="__l"></uni-load-more></scroll-view></block><block wx:if="{{webUser.rolename=='物业'?true:false}}"><scroll-view class="page scroll-Y" style="{{'height:'+(scrollviewHeight+'px')+';'}}" id="datasview" scroll-y="true" data-event-opts="{{[['scrolltolower',[['pageToLower',['$event']]]]]}}" bindscrolltolower="__e"><view class="cu-list menu-avatar"><block wx:for="{{recods}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="cu-item" data-name="{{item.name}}" data-id="{{item.id}}" data-event-opts="{{[['tap',[['openRecod',['$event']]]]]}}" bindtap="__e"><view class="{{['cu-avatar lg round ',item.state=='出'?'bg-red':'bg-brown']}}">{{item.state}}</view><view class="content"><view class="text-grey"><view class="text-cut">{{item.name}}</view><view class="cu-tag round bg-orange sm">{{item.temperature+" ℃"}}</view></view><view class="text-gray text-sm flex"><view class="text-cut"><text class="cuIcon-location text-red  margin-right-xs"></text>{{''+item.address+''}}</view></view></view><view><view class="text-grey text-xs">{{item.createDate}}</view></view></view></block></view><uni-load-more vue-id="3" status="{{loadStatus}}" bind:__l="__l"></uni-load-more></scroll-view></block></view>