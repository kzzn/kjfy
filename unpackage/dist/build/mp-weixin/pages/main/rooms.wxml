<view><cu-custom vue-id="1" isBack="{{true}}" bgColor="bg-shadeTop text-white" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">{{title}}</view></cu-custom><view><view data-event-opts="{{[['tap',[['addOwner']]]]}}" class="fab-box fab rightBottom" bindtap="__e"><view class="fab-circle bottom bg-cyan" style="right:0px;"><text class="cuIcon-add text-white xl" style="font-size:20px;"></text></view></view></view><scroll-view class="bg-white nav" id="fiexdview" scroll-x scroll-with-animation scroll-left="{{scrollLeft}}"><view class="flex text-center"><block wx:for="{{rooms}}" wx:for-item="room" wx:for-index="index" wx:key="id"><view class="{{['cu-item flex-sub',room.id==TabCur?'text-brown cur':'']}}" data-id="{{room.id}}" data-index="{{index}}" index="{{index}}" data-event-opts="{{[['tap',[['tabSelect',['$event']]]]]}}" bindtap="__e">{{''+room.name+''}}</view></block></view></scroll-view><scroll-view class="page scroll-Y" style="{{'height:'+(scrollviewHeight+'px')+';'}}" id="datasview" scroll-y="true" data-event-opts="{{[['scrolltolower',[['pageToLower',['$event']]]]]}}" bindscrolltolower="__e"><block wx:for="{{owners}}" wx:for-item="owner" wx:for-index="index" wx:key="index"><view class="cu-card article" id="{{owner.id}}"><view class="cu-item shadow" data-qrcode="{{owner.qrcode}}" data-userid="{{owner.id}}" data-event-opts="{{[['tap',[['openRecords',['$event']]]]]}}" bindtap="__e"><view class="title"><view class="text-cut">{{owner.name+" ("+owner.cardid+")"}}</view></view><view class="content"><image style="width:120px;" src="{{baseUrl+owner.qrcode}}" mode="aspectFit"></image><view class="desc"><view class="text-content"><view>{{"上次外出时间:"+owner.lastouttime+''}}</view><view>今日外出次数:<view class="cu-tag bg-brown light sm round">{{owner.outcount+"次"}}</view></view><view>今日外出时长:<view class="cu-tag bg-brown light sm round">{{owner.outtime}}</view></view></view><view><view class="cu-tag bg-brown light sm round">{{owner.age+"岁"}}</view><view class="cu-tag bg-green light sm round">{{owner.sex}}</view></view></view></view></view></view></block><uni-load-more vue-id="2" status="{{loadStatus}}" bind:__l="__l"></uni-load-more></scroll-view></view>