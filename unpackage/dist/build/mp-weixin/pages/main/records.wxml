<view><view class="fixed"><cu-custom vue-id="1" isBack="{{true}}" bgColor="bg-shadeTop text-white" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">出入登记</view></cu-custom></view><view class="flex solid-bottom justify-center"><view><block wx:if="{{requestParams.qrcode!=null?'true':false}}"><image style="width:180px;height:180px;" src="{{baseUrl+requestParams.qrcode}}" mode="aspectFill"></image></block></view></view><view class="cu-bar bg-white solid-bottom margin-top" id="titleview"><view class="action"><text class="cuIcon-title text-orange "></text>出入记录</view></view><scroll-view class="page" scroll-y><view class="cu-list menu-avatar"><block wx:for="{{recods}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="cu-item" data-name="{{item.name}}" data-id="{{item.id}}" data-event-opts="{{[['tap',[['openRecod',['$event']]]]]}}" bindtap="__e"><view class="{{['cu-avatar lg round ',item.state=='出'?'bg-red':'bg-brown']}}">{{item.state}}</view><view class="content"><view class="text-grey"><view class="text-cut">{{item.name}}</view><view class="cu-tag round bg-orange sm">{{item.temperature+" ℃"}}</view></view><view class="text-gray text-sm flex"><view class="text-cut"><text class="cuIcon-location text-red  margin-right-xs"></text>{{''+item.address+''}}</view></view></view><view class="action"><view class="text-grey text-xs">{{item.date}}</view><view class="cu-tag round bg-grey sm">{{item.time}}</view></view></view></block></view><uni-load-more vue-id="2" status="{{loadStatus}}" bind:__l="__l"></uni-load-more></scroll-view><block wx:if="{{webUser.rolename=='物业'?true:false}}"><view><view data-event-opts="{{[['tap',[['registerCode',['$event']]]]]}}" class="fab-box fab rightBottom" bindtap="__e"><view class="fab-circle bottom bg-brown" style="right:0px;"><text class="cuIcon-add text-white xl" style="font-size:20px;"></text></view></view></view></block></view>